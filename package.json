{
  "name": "react-redux-starter-kit",
  "version": "0.0.1",
  "description": "Get started with React, Redux, and React-Router!",
  "main": "index.js",
  "engines": {
    "node": ">=5.0.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "node -r dotenv/config --harmony bin/compile",
    "lint": "eslint . ./",
    "lint:fix": "npm run lint -- --fix",
    "start": "node -r dotenv/config bin/server --hot",
    "dev": "nodemon -r dotenv/config bin/server --hot",
    "dev:nw": "npm run dev -- --nw",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "node -r dotenv/config ./node_modules/karma/bin/karma start bin/karma.js",
    "test:dev": "npm run test -- --watch",
    "deploy": "npm run test && npm run clean && npm run compile",
    "demo": "npm run clean && NODE_ENV=production npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davezuko/react-redux-starter-kit.git"
  },
  "author": "David Zukowski <david@zuko.me> (http://zuko.me)",
  "license": "MIT",
